a=2
b=4
ans1=a+b
print("Addition of ",a,"&",b,"is",ans1)
ans2=a-b
print("Subtraction of ",a,"&",b,"is",ans2)
ans3=a*b
print("Multiplication of",a,"&",b,"is",ans3)
ans4=a/b
print("Division of",a,"&",b,"is",ans4)
ans5=a%b
print("Modulus of",a,"&",b,"is",ans5)
ans6=a//b
print("Floor Division of",a,"&",b,"is",ans6)




